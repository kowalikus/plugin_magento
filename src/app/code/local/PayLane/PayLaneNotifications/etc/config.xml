<?xml version="1.0" ?>
<config>
	<modules>
		<paylane_paylanenotifications>
			<version>0.1.0</version>
		</paylane_paylanenotifications>
	</modules>

	<global>
		<models>
			<paylanenotifications>
				<class>PayLane_PayLaneNotifications_Model</class>
			</paylanenotifications>
		</models>
		<blocks>
			<paylanenotifications>
				<class>PayLane_PayLaneNotifications_Block</class>
			</paylanenotifications>
		</blocks>
	</global>
 
	<default>
		<payment>
			<paylanenotifications>
                <model>paylanenotifications/standard</model>
				<sort_order>1005</sort_order>
				<active>0</active>
				<token></token>
			</paylanenotifications>
		</payment>
	</default> 
	<frontend>
		<routers>
			<paylanenotifications>
				<use>standard</use>
				<args>
					<module>PayLane_PayLaneNotifications</module>
					<frontName>paylanenotifications</frontName>
				</args>
			</paylanenotifications>
		</routers>
	</frontend>
</config>